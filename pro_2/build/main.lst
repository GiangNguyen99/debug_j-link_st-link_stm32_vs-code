  72:Core/Src/main.c ****   {
  73:Core/Src/main.c ****     return; /* requested port not enabled? */
  74:Core/Src/main.c ****   }
  75:Core/Src/main.c ****   timeout = 5000; /* arbitrary timeout value */
  76:Core/Src/main.c ****   while (ITM->PORT[0].u32 == 0)
  77:Core/Src/main.c ****   {
  78:Core/Src/main.c ****     /* Wait until STIMx is ready, then send data */
  79:Core/Src/main.c ****     if (--timeout == 0)
  80:Core/Src/main.c ****     {
  81:Core/Src/main.c ****       return; /* not able to send */
  82:Core/Src/main.c ****     }
  83:Core/Src/main.c ****   }
  84:Core/Src/main.c ****   ITM->PORT[0].u8 = (uint8_t)c;
  85:Core/Src/main.c **** }
