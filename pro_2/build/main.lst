  63:Core/Src/main.c ****   /* Check if Trace Control Register (ITM->TCR at 0xE0000E80) is set */
  64:Core/Src/main.c ****   /* check Trace Control Register if ITM trace is enabled*/
  65:Core/Src/main.c ****   if ((ITM->TCR & ITM_TCR_ITMENA_Msk) == 0)
  66:Core/Src/main.c ****   {
  67:Core/Src/main.c ****     return; /* not enabled? */
  68:Core/Src/main.c ****   }
  69:Core/Src/main.c ****   /* Check if the requested channel stimulus port (ITM->TER at 0xE0000E00) is enabled */
  70:Core/Src/main.c ****   /* check Trace Enable Register if requested port is enabled */
